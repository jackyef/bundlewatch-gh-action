    "@actions/exec": "^1.0.1",
    "@actions/github": "^1.1.0",
    "bundlewatch": "^0.2.5",
    "universal-user-agent": "^4.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.7.4",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.4",
    "jest": "^24.8.0",
    "jest-circus": "^24.7.1",
    "rollup": "^1.27.14",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-import-resolver": "^1.0.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.7.4"
  },
  "bundlewatch": {
